{
    "name": "@unified-api/vuejs-directory",
    "version": "1.1.12",
    "description": "VueJS component for Unified.to's Integrations Directory",
    "author": "Unified API Inc.",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/unified-to/unified-vuejs-integrations-directory/issues"
    },
    "homepage": "https://github.com/unified-to/unified-vuejs-integrations-directory#readme",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/unified-to/unified-vuejs-integrations-directory.git"
    },
    "keywords": [
        "unifiedapi",
        "vuejs"
    ],
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "./dist/UnifiedAPI-VueJS-Directory.umd.cjs",
    "module": "./dist/UnifiedAPI-VueJS-Directory.js",
    "types": "./dist/main.d.ts",
    "exports": {
        ".": {
            "import": "./dist/UnifiedAPI-VueJS-Directory.js",
            "require": "./dist/UnifiedAPI-VueJS-Directory.umd.js"
        }
    },
    "scripts": {
        "build": "vite build && vue-tsc --emitDeclarationOnly",
        "dev": "vite",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --noEmit",
        "publish": "npm publish"
    },
    "peerDependencies": {
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@types/node": "^18.15.11",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/tsconfig": "^0.1.3",
        "npm-run-all": "^4.1.5",
        "typescript": "~4.8.4",
        "vue-tsc": "^1.2.0",
        "vite": "^4.3.9"
    }
}
